public class DIspenser
{
	private int quantity;
	private String name;
	private String type;
	private String FoodInformation;

	public DIspenser(String newName, String newType, String newFoodInformation, int newQuantity)
	{
		this.name=newName;
		this.type=newType;
		this.FoodInformation=newFoodInformation;
		this.quantity=newQuantity;
	}
	public String getFoodInformation()
	{
		return FoodInformation;
	}
	public void setFoodInformation(String newFoodInformation)
	{
		newFoodInformation=FoodInformation;
	}
	public int getQuantity()
	{
		return quantity;
	}
	public String getName()
	{
		return name;
	}
	public String getType()
	{
		return type;
	}
	public void setQuantity(int newQuantity)
	{
		newQuantity=quantity;
	}
	public void setName(String newName)
	{
		newName=name;
	}
	public String toString()
	{
		return "Food Name: "+name+ "/n FoodType: "+type+"/n FoodQuantity: " +quantity;
	}
	public void setType(String newType)
	{
		newType=type;
	}

}



